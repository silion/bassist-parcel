{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
      }
    ],
    "mobx"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}